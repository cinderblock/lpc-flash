{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "noLib": false,
    "outDir": "lib/"
  },
  "exclude": [
    "lib",
    "node_modules",
    "test"
  ]
}
